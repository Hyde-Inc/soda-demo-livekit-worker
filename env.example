# PostgreSQL
POSTGRES_USER=app
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DB=app_db

# Redis
REDIS_USER=app
REDIS_PASSWORD=your_redis_password

# Foundry Authentication (required for uv sync during build)
FOUNDRY_TOKEN=your_foundry_token
UV_INDEX_FOUNDRY_MAIN_USERNAME=user
UV_INDEX_FOUNDRY_MAIN_PASSWORD=${FOUNDRY_TOKEN}
UV_INDEX_FOUNDRY_SDK_BUNDLE_USERNAME=user
UV_INDEX_FOUNDRY_SDK_BUNDLE_PASSWORD=${FOUNDRY_TOKEN}

# Foundry SDK (runtime)
FOUNDRY_HOSTNAME=hyde.palantirfoundry.com

# LiveKit
LIVEKIT_URL=wss://your-livekit-server.livekit.cloud
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret
SIP_OUTBOUND_TRUNK_ID=your_sip_trunk_id

# OpenAI
OPENAI_API_KEY=your_openai_api_key

# Deepgram
DEEPGRAM_API_KEY=your_deepgram_api_key

# Cartesia
CARTESIA_API_KEY=your_cartesia_api_key

# AWS (optional - for aws plugin)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=

# Egress Recording (S3 bucket for call recordings)
EGRESS_S3_BUCKET=your-recordings-bucket

# Sarvam (optional - for sarvam plugin)
SARVAM_API_KEY=
